            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.util.Random;

// Define the length of the random ID
int length = 10;

// Generate a random ID
StringBuilder randomUserIdBuilder = new StringBuilder();
Random random = new Random();
for (int i = 0; i &lt; length; i++) {
    // Generate a random character within the ASCII range for alphanumeric characters
    char randomChar = (char) (random.nextInt(26) + &apos;a&apos;);
    // Append the random character to the ID
    randomUserIdBuilder.append(randomChar);
}

// Set the random ID to a JMeter variable
vars.put(&quot;randomUserId&quot;, randomUserIdBuilder.toString());
//vars.put(&quot;randomUserId&quot;, &quot;yousef&quot;);</stringProp>
